{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js", "jquery-3.7.1.js", "saarthak.js", "matt.js", "kush.js", "kase.js"
      ],
      "matches": [
        "http://*/*", "https://*/*"
      ]
    }
  ]
}
